// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Property {
  id                 String  @unique() @default(cuid())
  title              String
  description        String
  coverImage         String
  cost               Decimal
  createdAt          String
  updatedAt          String
  openDate           String
  propertyCategoryId String
}
